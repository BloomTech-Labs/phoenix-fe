{
  "name": "phoenix-fe",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@babel/core": "^7.9.0",
    "@babel/preset-env": "^7.9.5",
    "@fortawesome/fontawesome-free": "^5.12.1",
    "@material-ui/core": "^4.9.5",
    "@material-ui/icons": "^4.9.1",
    "@testing-library/user-event": "^10.0.2",
    "@types/react-transition-group": "^4.2.4",
    "axios": "^0.19.2",
    "babel-jest": "^24.7.0",
    "jest": "^24.9.0",
    "jest-dom": "^4.0.0",
    "jsonwebtoken": "^8.5.1",
    "moment": "^2.24.0",
    "react": "^16.13.0",
    "react-addons-css-transition-group": "^15.6.2",
    "react-awesome-slider": "^4.1.0",
    "react-dom": "^16.12.0",
    "react-ga": "^2.7.0",
    "react-hook-form": "^5.1.1",
    "react-router-dom": "^5.1.2",
    "react-scripts": "3.4.1",
    "react-test-renderer": "^16.13.1"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "coverage": "CI=true npm test -- --coverage --watchAll=false || true"
  },
  "jest": {
    "transform": {
      "^.+\\.(js|jsx)$": "babel-jest"
    },
    "coverageThreshold": {
      "global": {
        "branches": 40,
        "functions": 40,
        "lines": 40,
        "statements": 40
      },
      "./src/App.js": {
        "lines": 100
      }
    },
    "collectCoverageFrom": [
      "./src/*.js",
      "./src/components/*.js",
      "./src/components/**/*.js",
      "**/*.{js}",
      "!**/node_modules/**",
      "!**/vendor/**",
      "!**/coverage/**",
      "!**__tests__",
      "!**/**/Terms/*.js",
      "!/../components/Terms/*.js"
    ]
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/preset-react": "^7.9.4",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.5.0",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "react-test-renderer": "^16.13.1"
  }
}
